    "react-native-modal-picker": "0.0.16",
    "react-native-scrollable-tab-view": "^0.7.0",
    "react-native-vector-icons": "^4.0.0"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "jest": "18.0.0",
